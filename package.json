{
  "name": "udomo_webapplication",
  "author": "Lucas Vinals",
  "license": "GPL-3.0",
  "main": "cluster.js",
  "version": "1.0.0",
  "scripts": {
    "start": "node cluster.js -c 1",
    "cluster": "node cluster.js",
    "clean": "rimraf udomo *.log",
    "production": "npm run clean && NODE_ENV=production webpack",
    "development": "NODE_ENV=development node dev-server.js",
    "prepareSystem": "bash ./server/tools/prepareSystem.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasvinals/uDomo"
  },
  "dependencies": {
    "alertifyjs": "^1.9.0",
    "angular": "^1.6.3",
    "angular-ui-router": "^0.4.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "helmet": "^3.5.0",
    "http-status": "^1.0.1",
    "jquery": "^3.1.1",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "method-override": "^2.3.8",
    "mongoose": "^4.9.2",
    "mongoose-delete": "^0.3.4",
    "optimist": "^0.6.1",
    "socket.io": "^1.7.3",
    "socket.io-adapter-mongo": "^0.1.6",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.17.1",
    "eslint-config-strict": "^13.0.0",
    "file-loader": "^0.11.1",
    "glob": "^7.1.1",
    "google-closure-compiler-js": "^20170409.0.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.0.4",
    "gulp-concat": "^2.6.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-processhtml": "^1.1.0",
    "mocha": "^3.2.0",
    "morgan": "^1.8.1",
    "psi": "^3.0.0",
    "require-dir": "^0.3.1",
    "rimraf": "^2.6.1",
    "run-sequence": "^1.2.2",
    "style-loader": "^0.16.1",
    "syntax-error": "^1.3.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "eslintConfig": {
    "extends": [
      "strict/es6"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "globals": {
      "angular": true,
      "_": true
    },
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "rules": {
      "no-process-env": [
        0
      ],
      "filenames/match-regex": 0,
      "id-length": [
        2,
        {
          "min": 2,
          "max": 100,
          "properties": "always",
          "exceptions": [
            "_"
          ]
        }
      ],
      "id-match": [
        2,
        "^([A-Za-z-_$]*|[A-Z][A-Z_]*[A-Z])$",
        {
          "properties": false
        }
      ]
    }
  }
}
