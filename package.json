{
  "name": "udomo_webapplication",
  "author": "Lucas Vinals",
  "licenses": [
    {
      "type": "GPL-3.0",
      "url": "http://www.gnu.org/licenses/gpl-3.0-standalone.html"
    }
  ],
  "main": "cluster.js",
  "version": "1.0.0",
  "scripts": {
    "start": "gulp build && ./server/binaries/nodejs/bin/node ./cluster.js 12078 1",
    "cluster": "gulp build && ./server/binaries/nodejs/bin/node ./cluster.js 12078",
    "prepare": "bash ./server/tools/prepareSystem.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasvinals/uDomo"
  },
  "dependencies": {
    "alertifyjs": "^1.9.0",
    "angular": "^1.6.3",
    "angular-ui-router": "^0.4.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "fs-extra": "^2.0.0",
    "jquery": "^3.1.1",
    "jsonwebtoken": "^7.3.0",
    "know-your-http-well": "^0.5.0",
    "lodash": "^4.17.4",
    "method-override": "^2.3.7",
    "mongoose": "^4.8.6",
    "mongoose-delete": "^0.3.4",
    "socket.io": "^1.7.3",
    "socket.io-adapter-mongo": "^0.1.6",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "eslint": "^3.17.1",
    "eslint-config-strict": "^13.0.0",
    "glob": "^7.1.1",
    "google-closure-compiler-js": "^20170218.0.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.0.3",
    "gulp-concat": "^2.6.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-processhtml": "^1.1.0",
    "mocha": "^3.2.0",
    "morgan": "^1.8.1",
    "psi": "^3.0.0",
    "syntax-error": "^1.3.0"
  },
  "eslintConfig": {
    "extends": [
      "strict/es6"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "rules": {
      "id-length": [
        2,
        {
          "min": 2,
          "max": 100,
          "properties": "always",
          "exceptions": [
            "_"
          ]
        }
      ],
      "id-match": [
        2,
        "^([A-Za-z-_]*|[A-Z][A-Z_]*[A-Z])$",
        {
          "properties": false
        }
      ]
    }
  }
}
