{
  "name": "udomo",
  "author": "Lucas Vinals",
  "license": "GPL-3.0",
  "main": "cluster.js",
  "version": "1.0.0",
  "scripts": {
    "start": "CORES=1 nodemon --inspect --watch server -- ./cluster.js",
    "cluster": "nodemon -- ./cluster.js",
    "clean": "rimraf ./udomo dump.rdb *.log .com.google* v8*",
    "copystatic": "mkdir -p ./udomo/views && cp -R ./client/views/**/ ./udomo/views/",
    "basicWorkflow": "yarn run clean && yarn run copystatic",
    "build:production": "yarn run basicWorkflow && webpack -p",
    "production": "NODE_ENV=production concurrently --kill-others-on-fail --raw 'yarn run build:production' 'yarn run cluster'",
    "development": "NODE_ENV=development concurrently --kill-others --raw 'yarn start' 'webpack-dev-server'",
    "snyk-auth": "snyk auth",
    "snyk-test": "snyk test -q",
    "snyk-protect": "snyk protect -q"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasvinals/uDomo"
  },
  "dependencies": {
    "alertifyjs": "^1.9.0",
    "angular": "^1.6.3",
    "angular-ui-router": "^1.0.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "bootstrap": "^3.3.7",
    "compression": "^1.6.2",
    "concurrently": "^3.4.0",
    "express": "^4.15.3",
    "helmet": "^3.6.1",
    "http-status": "^1.0.1",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "method-override": "^2.3.9",
    "mongoose": "^4.10.2",
    "mongoose-delete": "^0.3.4",
    "ng-annotations": "^1.1.0",
    "snyk": "^1.34.3",
    "socket.io": "^2.0.3",
    "socket.io-redis": "^5.1.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap-loader": "^2.1.0",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^0.28.4",
    "eslint": "^4.0.0",
    "eslint-config-strict": "^13.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "glob": "^7.1.2",
    "google-closure-compiler-js": "^20170521.0.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-processhtml": "^1.1.0",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.2.1",
    "mocha": "^3.4.2",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "psi": "^3.0.0",
    "raw-loader": "^0.5.1",
    "require-dir": "^0.3.1",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "run-sequence": "^1.2.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "syntax-error": "^1.3.0",
    "url-loader": "^0.5.8",
    "webpack": "^3.0.0",
    "webpack-dev-server": "2.4.5"
  },
  "eslintConfig": {
    "extends": [
      "strict"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "globals": {
      "DEVELOPMENT": true,
      "PRODUCTION": true,
      "PORT": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "rules": {
      "no-process-env": [
        0
      ],
      "filenames/match-regex": 0,
      "id-length": [
        2,
        {
          "min": 2,
          "max": 100,
          "properties": "always",
          "exceptions": [
            "_"
          ]
        }
      ],
      "id-match": [
        2,
        "^([A-Za-z-_$]*|[A-Z][A-Z_]*[A-Z])$",
        {
          "properties": false
        }
      ]
    }
  },
  "snyk": true
}
